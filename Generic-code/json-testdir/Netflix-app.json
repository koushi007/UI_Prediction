{
    "app-name": "Netflix",
    "directory-path" : "Netflix-Testing/",
    "icon-images-path" : "org.tizen.netflix-app",
    "icon-images-mapping" : "Netflix_iconstates.csv",
    "states-mapping" : "Netflix_statesmap.csv",
    "app-identifier" : 1,
    "search-icon-global" : [
        {
            "threshold-value" : 230,
            "threshold-type" : 0,
            "icon-parameters" : [0,100,80,470,"menu"]
        }
    ],
    "focus-boxes" : [
        {
            "blurring" : 0,
            "threshold-value" : 230,
            "threshold-type" : 0,
            "rectangle-error" : 0.02,
            "conditions": [ "w > 15" , "h > 15"],
            "search-icon": [
                {
                    "icon-conditions" : ["y>5","x > 60"],
                    "icon-parameters" : ["x-60","y-5","65","h+10","home"]
                },
                {
                    "icon-conditions" : ["y>200", "w>50", "h<40", "x > 0"],
                    "icon-parameters" : ["x","y","w","h","content"]
                }
            ],
            "color": [255,0,0]
        },
        {
            "blurring" : 0,
            "threshold-value" : 0,
            "threshold-type" : 1,
            "rectangle-error" : 0.02,
            "conditions": [ "w > 15" , "h > 15" , "w<200"],
            "search-icon" : [],
            "color": [0,255,0]
        }
    ]
}